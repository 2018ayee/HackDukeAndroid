        android:background="@android:color/transparent"
        android:clickable="true"
        android:shadowColor="#FFFFFF"
        android:text="hackduke-2019.devpost.com"
        android:textAllCaps="false"
        android:textColor="#1676FF"
        app:layout_constraintEnd_toEndOf="@+id/generalCommunication"
        app:layout_constraintStart_toEndOf="@+id/devpostText"
