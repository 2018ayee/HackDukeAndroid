        android:layout_width="wrap_content"
        android:layout_height="20sp"
        android:layout_marginStart="120dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="26dp"
        android:layout_marginRight="26dp"
        android:background="@android:color/transparent"
        android:clickable="true"
        android:shadowColor="#FFFFFF"
        android:text="911 (Emergency)"
        android:textAllCaps="false"
        android:textColor="@android:color/black"
        app:layout_constraintEnd_toEndOf="@+id/emergency"
        app:layout_constraintStart_toEndOf="@+id/durhamPoliceText"
        app:layout_constraintTop_toTopOf="@+id/emergency"
        tools:fontFamily="sans-serif" />

    <TextView
        android:id="@+id/vickisCell"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="13dp"
        android:text="Vicki's Cell Number:"
        android:textColor="@android:color/black"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@+id/generalCommunication"
        app:layout_constraintTop_toBottomOf="@+id/samsCell"
        tools:fontFamily="sans-serif" />

    <Button
        android:id="@+id/vickisNumber"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="wrap_content"
        android:layout_height="20sp"
        android:layout_marginStart="90dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="28dp"
        android:layout_marginRight="28dp"
        android:background="@android:color/transparent"
        android:clickable="true"
        android:shadowColor="#FFFFFF"
        android:text="(908) 938-8667"
        android:textColor="#1676FF"
        app:layout_constraintEnd_toEndOf="@+id/generalCommunication"
        app:layout_constraintStart_toEndOf="@+id/vickisCell"
        app:layout_constraintTop_toTopOf="@+id/generalCommunication"
        tools:fontFamily="sans-serif" />

    <TextView
        android:id="@+id/generalCommunicationText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="8dp"
        android:text="General Communication"
        android:textColor="@android:color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/generalCommunication"
        app:layout_constraintTop_toTopOf="@+id/generalCommunication"
        tools:fontFamily="sans-serif-black" />

    <TextView
        android:id="@+id/samsCell"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="13dp"
        android:text="Sam's Cell Number:"
        android:textColor="@android:color/black"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@+id/generalCommunication"
        app:layout_constraintTop_toBottomOf="@+id/generalCommunicationText"
        tools:fontFamily="sans-serif" />

    <Button
        android:id="@+id/samsNumber"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="wrap_content"
        android:layout_height="20sp"
        android:layout_marginStart="90dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="25dp"
        android:layout_marginRight="25dp"
        android:background="@android:color/transparent"
        android:clickable="true"
        android:shadowColor="#FFFFFF"
        android:text="(410) 428-2378"
        android:textColor="#1676FF"
        app:layout_constraintEnd_toEndOf="@+id/generalCommunication"
        app:layout_constraintStart_toEndOf="@+id/samsCell"
        app:layout_constraintTop_toTopOf="@+id/generalCommunication"
        tools:fontFamily="sans-serif" />

    <TextView
        android:id="@+id/slackText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
