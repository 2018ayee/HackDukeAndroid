        android:layout_height="0dp"
        android:layout_marginStart="18dp"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="18dp"
        android:layout_marginRight="18dp"
        android:layout_marginBottom="18dp"
        android:fillViewport="true"
        android:scrollbarAlwaysDrawVerticalTrack="false"
        android:scrollbarFadeDuration="250"
        android:scrollbarSize="3dp"
        android:scrollbarStyle="insideOverlay"
        android:scrollbars="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/generalCommunication">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/FAQsText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="13dp"
                android:layout_marginLeft="13dp"
                android:layout_marginTop="8dp"
                android:text="FAQs"
                android:textColor="@android:color/black"
                android:textSize="16sp"
                android:textStyle="bold"
                tools:fontFamily="sans-serif-black"
                tools:visibility="visible" />

            <TextView
                android:id="@+id/textView28"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/prizes"
                android:layout_width="328dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="13dp"
                android:layout_marginLeft="13dp"
                android:text="Prizes?"
                android:textColor="@android:color/black"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:fontFamily="sans-serif" />

            <TextView
                android:id="@+id/prizesText"
                android:layout_width="328dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="13dp"
                android:layout_marginLeft="13dp"
                android:text="The grand prizes are donations under your name to charities and causes! There will be grand prize winners for each track, novice track and many other company sponsored API prizes as well."
                android:textColor="@android:color/black"
                android:textSize="15sp"
                tools:fontFamily="sans-serif" />

            <TextView
                android:id="@+id/whatMake"
                android:layout_width="328dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="18dp"
                android:layout_marginLeft="13dp"
                android:layout_marginTop="13dp"
                android:text="What can I make?"
                android:textColor="@android:color/black"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:fontFamily="sans-serif" />

            <TextView
                android:id="@+id/whatText"
                android:layout_width="328dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="18dp"
                android:layout_marginLeft="13dp"
                android:layout_marginTop="13dp"
                android:text="Bring your own ethernet cables (or you can just use WiFi) and bring any Nerf gun arsenal you have!"
                android:textColor="@android:color/black"
                android:textSize="15sp"
                tools:fontFamily="sans-serif" />

            <TextView
                android:id="@+id/who"
                android:layout_width="328dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="18dp"
                android:layout_marginLeft="13dp"
                android:layout_marginTop="13dp"
                android:text="Who can I work with?"
                android:textColor="@android:color/black"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:fontFamily="sans-serif" />

            <TextView
                android:id="@+id/whoText"
                android:layout_width="328dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="18dp"
                android:layout_marginLeft="13dp"
                android:layout_marginTop="13dp"
                android:text="Anyone! Get to know your fellow programmers better from your own and other schools! The only request is that you make a new friend by talking to each other, our mentors and our experts!"
                android:textColor="@android:color/black"
                android:textSize="15sp"
                tools:fontFamily="sans-serif" />

            <TextView
                android:id="@+id/whatMakeText"
                android:layout_width="328dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="18dp"
                android:layout_marginLeft="13dp"
                android:layout_marginTop="13dp"
                android:text="We welcome all software and hardware hacks that align to our tracks for impact. We will also have lab spaces for hardware hackers."
                android:textColor="@android:color/black"
                android:textSize="15sp"
                tools:fontFamily="sans-serif" />

            <TextView
                android:id="@+id/what"
                android:layout_width="328dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="18dp"
                android:layout_marginLeft="13dp"
                android:layout_marginTop="13dp"
                android:text="What to Bring?"
                android:textColor="@android:color/black"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:fontFamily="sans-serif" />

            <TextView
                android:id="@+id/whenWhere"
                android:layout_width="328dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="18dp"
                android:layout_marginLeft="13dp"
                android:layout_marginTop="13dp"
                android:text="When? Where?"
                android:textColor="@android:color/black"
                android:textSize="15sp"
                android:textStyle="bold"
                tools:fontFamily="sans-serif" />

            <TextView
                android:id="@+id/whenWhereText"
                android:layout_width="328dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="18dp"
                android:layout_marginLeft="13dp"
                android:layout_marginTop="13dp"
                android:text="HackDuke starts Saturday, Nov 2nd at 9:00 AM and ends on Sunday,m Nov 3rd at 5:30 PM. It is hosted on Duke University's West Campus, in and around the Fitzpatrick Engineering Building."
                android:textColor="@android:color/black"
                android:textSize="15sp"
                tools:fontFamily="sans-serif" />

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

    <TextView
        android:id="@+id/emergencyText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="18dp"
        android:text="Emergency"
        android:textColor="@android:color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/dukePoliceText"
        app:layout_constraintEnd_toEndOf="@+id/emergency"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/emergency"
        app:layout_constraintTop_toTopOf="@+id/emergency"
        tools:fontFamily="sans-serif-black" />

    <TextView
        android:id="@+id/dukePoliceText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="13dp"
        android:text="Duke Police"
        android:textColor="@android:color/black"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@+id/emergency"
        app:layout_constraintTop_toBottomOf="@+id/emergencyText"
        tools:fontFamily="sans-serif" />

    <Button
