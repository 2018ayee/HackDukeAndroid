        android:textColor="#1676FF"
        app:layout_constraintEnd_toEndOf="@+id/generalCommunication"
        app:layout_constraintHorizontal_bias="0.94"
        app:layout_constraintStart_toEndOf="@+id/slackText"
        app:layout_constraintTop_toBottomOf="@+id/vickisNumber"
        tools:fontFamily="sans-serif" />

    <TextView
        android:id="@+id/devpostText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="13dp"
        android:text="Devpost Link:"
        android:textColor="@android:color/black"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="@+id/generalCommunication"
